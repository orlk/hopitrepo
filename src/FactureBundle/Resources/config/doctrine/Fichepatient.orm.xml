<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FactureBundle\Entity\Fichepatient" table="fichepatient">
    <indexes>
      <index name="codeDm" columns="codeDm"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="taille" type="float" column="taille" precision="10" scale="0" nullable="false"/>
    <field name="poid" type="float" column="poid" precision="10" scale="0" nullable="false"/>
    <field name="tension" type="float" column="tension" precision="10" scale="0" nullable="false"/>
    <field name="temperature" type="float" column="temperature" precision="10" scale="0" nullable="false"/>
    <field name="perimetre" type="float" column="perimetre" precision="10" scale="0" nullable="false"/>
    <field name="groupeSanguin" type="string" column="groupe_sanguin" length="30" nullable="false"/>
    <field name="codedm" type="integer" column="codeDm" nullable="true"/>
    <field name="datecreation" type="date" column="dateCreation" nullable="false"/>
  </entity>
</doctrine-mapping>
