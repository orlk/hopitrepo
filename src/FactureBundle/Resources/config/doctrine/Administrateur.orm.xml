<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FactureBundle\Entity\Administrateur" table="administrateur">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="super" type="integer" column="super" nullable="false"/>
    <field name="cin" type="string" column="cin" length="10" nullable="false"/>
    <field name="nom" type="string" column="nom" length="50" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="telephone" type="integer" column="telephone" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="200" nullable="false"/>
    <field name="login" type="string" column="login" length="50" nullable="false"/>
    <field name="password" type="string" column="password" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
